{
  "name": "KeystoneJS Starter",
  "description": "A basic KeystoneJS website with a blog, image gallery and contact form.",
  "keywords": [
    "keystone",
    "keystonejs",
    "mongo",
    "mongodb",
    "mongoose",
    "express",
    "jade",
    "blog",
    "cms",
    "website"
  ],
  "website": "https://keystonejs.com",
  "repository": "https://github.com/maryam4420/keystone-starter",
  "logo": "http://keystonejs.com/images/logo-128.png",
  "env" : {
    "NODE_ENV": "production",
    "COOKIE_SECRET": {
      "description": "Secret for signing cookies",
      "generator": "secret"
    },
    "EMAIL_HOSTNAME": {
      "description": "Canonical Hostname for images and links in emails",
      "value": "",
      "required": false
    },
    "PERSON_NAME": {
      "description": "Person or Company Name that appears on the website e.g. your first or full name",
      "value": "",
      "required": false
    }
   },
   "addons": [
    "mongolab:sandbox",
    "cloudinary:starter",
    "papertrail:choklad"
  ]
}
